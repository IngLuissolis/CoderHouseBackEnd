<h1>Carrito Nro {{carts._id}}</h1>
<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Precio unitario</th>
      <th>Cantidad</th>
    </tr>
  </thead>
  <tbody>
    {{#each carts.products}}
      <tr>
        <td>{{this.product.nombre}}</td>
        <td>{{this.product.camiseta1Precio}}</td>
        <td>{{this.quantity}}</td>
      </tr>
    {{/each}}
  </tbody>
</table>